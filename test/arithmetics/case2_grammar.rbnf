 
Mul  ::= !lhs=Mul !op=("*" | "/") !rhs=Atom        -> ArithCall(op, lhs, rhs);
Mul  ::= !a=Atom                                   -> a;
Add  ::= !lhs=Add !op=("+" | "-") !rhs=Mul         -> ArithCall(op, lhs, rhs);
Add  ::= !a=Mul                                    -> a;
Atom ::= "(" !a=Add ")"                            -> a;
Atom ::= !a=<number>                               -> unwrap(a);

START  ::= <BOF> !a=Add <EOF>                        -> a;
